    "@polkadot/api": "^8.6.2",
    "@polkadot/extension-dapp": "^0.43.2",
    "antd": "^4.6.3",
    "classnames": "^2.2.6",
    "dayjs": "^1.9.7",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "6",
    "recharts": "^2.1.10",
    "spark-md5": "^3.0.1",
    "unchanged": "^2.2.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "husky": "^8.0.0",
    "less": "^4.1.2",
    "lint-staged": "^13.0.0",
    "prettier": "^2.6.2",
    "vite": "^2.9.9"
  }
